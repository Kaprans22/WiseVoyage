<div class="container mt-5 pt-5">
<div>
  <h1>Explore <%= @suggestion.title %> </h1>
</div>
<div>
  <% @suggestion.photos.each do |photo| %>
  <%= cl_image_tag photo.key, height: 300, width: 400 %>
<% end %>
</div>
<div>
  <h2> <%= @suggestion.subtitle %></h2>
  <p> <%= @suggestion.paragraph1 %> </p>
  <h4><%= @suggestion.author %></h4>
  <h5><%= @suggestion.quote %></h5>
  <p> <%= @suggestion.paragraph2 %> </p>
  <p> <%= @suggestion.paragraph3 %> </p>
</div>
</div>
